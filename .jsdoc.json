{
    "tags": {
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["src/", "package.json", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "K of C 7186 Fish Fry: Google & Square",
        "disableSort": false
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./node_modules/@freshes/jsdoc-template"
    }
}
